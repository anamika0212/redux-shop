{
  "name": "redux-shop",
  "version": "1.0.0",
  "description": "A Shopping application using Redux.",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anamika0212/redux-shop.git"
  },
  "author": "Anamika Jha",
  "license": "ISC",
  "dependencies": {
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "bugs": {
    "url": "https://github.com/anamika0212/redux-shop/issues"
  },
  "homepage": "https://github.com/anamika0212/redux-shop#readme"
}
